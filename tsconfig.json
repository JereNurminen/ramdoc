{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES6",
    "moduleResolution": "node",
    "rootDir": "src/",
    "outDir": "build/",
    "lib": [ "DOM", "ES6"  ],
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "strictNullChecks": false,
    "noUnusedLocals": true
  }
}
